<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="ar.edu.fi.unju.model.domian.Cliente" table="Clientes">
			<id name = "idCliente" type="long">
				<column name="id_cliente"></column>
				<generator class="increment"></generator>
			</id>
			<property name="nombre" type="string">
				<column name="nombre"></column>
			</property>
			<property name="apellido" type="string">
				<column name="apellido"></column>
			</property>
			<property name="dni" type="long">
				<column name="dni"></column>
			</property>
	</class>

</hibernate-mapping>
